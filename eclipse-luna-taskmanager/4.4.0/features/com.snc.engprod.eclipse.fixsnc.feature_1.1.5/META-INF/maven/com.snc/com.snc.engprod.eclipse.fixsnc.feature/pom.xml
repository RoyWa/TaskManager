			<artifactId>com.snc.engprod.eclipse.FixSncPlugin</artifactId>
			<type>eclipse-plugin</type>
			<version>${fixsnc.version}</version>
		</dependency>
	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-deploy-plugin</artifactId>
				<configuration>
					<skip>true</skip>
				</configuration>
			</plugin>
		</plugins>
