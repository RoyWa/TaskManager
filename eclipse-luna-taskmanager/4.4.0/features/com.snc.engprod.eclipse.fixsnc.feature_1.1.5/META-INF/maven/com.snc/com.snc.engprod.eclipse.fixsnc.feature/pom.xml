	<artifactId>com.snc.engprod.eclipse.fixsnc.feature</artifactId>
	<version>1.1.5</version>
	<packaging>eclipse-feature</packaging>

	<scm>
		<connection>scm:git:ssh://source.devsnc.com/dev-tools.git</connection>
		<developerConnection>scm:git:ssh://source.devsnc.com/dev-tools.git</developerConnection>
		<tag>master</tag>
		<url>https://source.devsnc.com/view/?p=dev-tools.git</url>
	</scm>
	
	<properties>
		<fixsnc.version>1.1.5-SNAPSHOT</fixsnc.version>
